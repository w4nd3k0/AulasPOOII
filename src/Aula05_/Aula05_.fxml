<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="Aula05_.Aula05_Controller">
   <children>
      <MenuBar prefHeight="32.0" prefWidth="600.0">
         <menus>
            <Menu mnemonicParsing="false" text="Arquivo">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#Imprimir" text="Imprimir na Saída">
                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="Q" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
                  <MenuItem mnemonicParsing="false" onAction="#NovaJanela" text="Nova janela" />
                  <Menu mnemonicParsing="false" text="Colar">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Especial" />
                    </items>
                  </Menu>
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Exportar">
              <items>
                <MenuItem mnemonicParsing="false" text="Ainda Não" />
              </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</AnchorPane>
